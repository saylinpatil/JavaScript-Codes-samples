let arr=[3,5,22,43,65,87,9,5,66,75,8,2,7];

let temp;

for(let i=0;i<arr.length;i++){
  for(let j=i+1;j<arr.length;j++){
      
     if(arr[i]>arr[j])
  
      {
         temp=arr[i];
         arr[i]=arr[j];
         arr[j]=temp;
      }
    }
}

console.log(arr);

Output:

[
   2,  3,  5,  5,  7,  8,
   9, 22, 43, 65, 66, 75,
  87
]
