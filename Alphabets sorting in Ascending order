let arr = ["g", "k", "b", "a", "d", "v"];

for(let i=0; i<arr.length; i++) {
    for(let j=i+1; j<arr.length; j++) {
        if(arr[i] > arr[j]) {
            let temp = arr[j];
            arr[j] = arr[i];
            arr[i] = temp;
        }
    }
}
console.log(arr)

Output-
[ 'a', 'b', 'd', 'g', 'k', 'v' ]
